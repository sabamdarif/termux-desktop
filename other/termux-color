# Main TUI function - Two column layout
show_menu() {
    local search_term=""
    local selected_row=0
    local selected_col=0
    local scroll_offset=0
    local schemes=()
    # Calculate available screen height dynamically
    local term_height=$(tput lines)
    local visible_rows=$((term_height - 7)) # Reserve 7 lines for header/footer (3 header + 4 footer)
    ((visible_rows < 5)) && visible_rows=5  # Minimum 5 rows

    # Load all schemes into array
    while IFS= read -r scheme; do
        schemes+=("$scheme")
    done < <(get_schemes)

    while true; do
        # Filter schemes based on search term
        local filtered_schemes=()
        for scheme in "${schemes[@]}"; do
            [[ -z "$search_term" ]] || [[ "${scheme,,}" == *"${search_term,,}"* ]] && filtered_schemes+=("$scheme")
        done

        local total_filtered=${#filtered_schemes[@]}
        local total_rows=$(((total_filtered + 1) / 2))

        # Boundary checks
        ((selected_row >= total_rows)) && selected_row=$((total_rows - 1))
        ((selected_row < 0)) && selected_row=0

        # Check if selected column is valid for current row
        local idx=$((selected_row * 2 + selected_col))
        if ((idx >= total_filtered)); then
            selected_col=0
        fi

        # Auto-scroll logic
        if ((selected_row < scroll_offset)); then
            scroll_offset=$selected_row
        elif ((selected_row >= scroll_offset + visible_rows)); then
            scroll_offset=$((selected_row - visible_rows + 1))
        fi

        # Ensure scroll offset is valid
        local max_scroll=$((total_rows - visible_rows))
        ((max_scroll < 0)) && max_scroll=0
        ((scroll_offset > max_scroll)) && scroll_offset=$max_scroll
        ((scroll_offset < 0)) && scroll_offset=0

        # Clear screen and draw UI
        clear

        # Compact header
        echo "╭─ TERMUX COLOR SWITCHER ──────────────────────────────────────────────╮"
        echo "  🔍 ${search_term}_"
        echo "╰──────────────────────────────────────────────────────────────────────╯"

        # Show schemes in two columns
        if ((total_filtered == 0)); then
            echo "  No matches found"
        else
            local end_row=$((scroll_offset + visible_rows))
            ((end_row > total_rows)) && end_row=$total_rows

            for ((row = scroll_offset; row < end_row; row++)); do
                local left_idx=$((row * 2))
                local right_idx=$((row * 2 + 1))

                # Left column
                if ((left_idx < total_filtered)); then
                    local left_scheme="${filtered_schemes[left_idx]}"
                    if ((row == selected_row && selected_col == 0)); then
                        printf "  \033[1;36m▶ %-32s\033[0m" "$left_scheme"
                    else
                        printf "    %-32s" "$left_scheme"
                    fi
                else
                    printf "    %-32s" ""
                fi

                # Right column
                if ((right_idx < total_filtered)); then
                    local right_scheme="${filtered_schemes[right_idx]}"
                    if ((row == selected_row && selected_col == 1)); then
                        printf "  \033[1;36m▶ %-32s\033[0m\n" "$right_scheme"
                    else
                        printf "    %-32s\n" "$right_scheme"
                    fi
                else
                    echo ""
                fi
            done

            # Fill remaining lines to reach full screen
            local shown=$((end_row - scroll_offset))
            for ((i = shown; i < visible_rows; i++)); do
                printf "  %-32s  %-32s\n" "" ""
            done
        fi

        echo "╭────────────────────────────────────────────────────────────────────────╮"

        # Status bar
        if ((total_filtered > 0)); then
            local current_idx=$((selected_row * 2 + selected_col))
            local pos=$((current_idx + 1))
            printf "  %d/%d • ↑↓←→:Navigate PgUp/PgDn:Scroll ⏎:Apply ^U:Clear ESC:Exit\n" "$pos" "$total_filtered"

            # Live preview - apply current selection
            local preview_idx=$((selected_row * 2 + selected_col))
            if ((preview_idx < total_filtered)); then
                apply_colors "${filtered_schemes[preview_idx]}"
            fi
        else
            echo "  Type to search • ESC: Exit"
        fi
        echo "╰────────────────────────────────────────────────────────────────────────╯"

        # Read input
        IFS= read -rsn1 key
        case "$key" in
        $'\033') # ESC or arrow keys
            read -rsn2 -t 0.1 key2
            if [[ -z "$key2" ]]; then
                # Standalone ESC - exit
                clear
                restore_colors
                exit 0
            else
                case "$key2" in
                '[A') # Up arrow
                    ((selected_row > 0)) && ((selected_row--))
                    ;;
                '[B') # Down arrow
                    if ((selected_row < total_rows - 1)); then
                        ((selected_row++))
                        # Check if new position is valid
                        local check_idx=$((selected_row * 2 + selected_col))
                        ((check_idx >= total_filtered)) && selected_col=0
                    fi
                    ;;
                '[D') # Left arrow
                    if ((selected_col == 1)); then
                        selected_col=0
                    elif ((selected_row > 0)); then
                        ((selected_row--))
                        # Move to right column of previous row if it exists
                        local check_idx=$((selected_row * 2 + 1))
                        if ((check_idx < total_filtered)); then
                            selected_col=1
                        else
                            selected_col=0
                        fi
                    fi
                    ;;
                '[C') # Right arrow
                    local next_idx=$((selected_row * 2 + 1))
                    if ((selected_col == 0 && next_idx < total_filtered)); then
                        selected_col=1
                    elif ((selected_row < total_rows - 1)); then
                        ((selected_row++))
                        selected_col=0
                    fi
                    ;;
                '[5')                 # Page Up
                    read -rsn1 -t 0.1 # consume ~
                    ((selected_row -= visible_rows))
                    ((selected_row < 0)) && selected_row=0
                    ;;
                '[6')                 # Page Down
                    read -rsn1 -t 0.1 # consume ~
                    ((selected_row += visible_rows))
                    ((selected_row >= total_rows)) && selected_row=$((total_rows - 1))
                    # Validate column
                    local check_idx=$((selected_row * 2 + selected_col))
                    ((check_idx >= total_filtered)) && selected_col=0
                    ;;
                '[H') # Home
                    selected_row=0
                    selected_col=0
                    ;;
                '[F') # End
                    selected_row=$((total_rows - 1))
                    local last_idx=$((total_filtered - 1))
                    selected_col=$((last_idx % 2))
                    ;;
                esac
            fi
            ;;
        '') # Enter
            if ((total_filtered > 0)); then
                local apply_idx=$((selected_row * 2 + selected_col))
                if ((apply_idx < total_filtered)); then
                    clear
                    echo "✓ Applied: ${filtered_schemes[apply_idx]}"
                    rm -f "$BACKUP_FILE" 2>/dev/null
                    exit 0
                fi
            fi
            ;;
        $'\x7f' | $'\x08') # Backspace
            if [[ -n "$search_term" ]]; then
                search_term="${search_term%?}"
                selected_row=0
                selected_col=0
                scroll_offset=0
            fi
            ;;
        $'\x15') # Ctrl+U - clear search
            search_term=""
            selected_row=0
            selected_col=0
            scroll_offset=0
            ;;
        *)
            if [[ -n "$key" && "$key" =~ [[:print:]] ]]; then
                search_term+="$key"
                selected_row=0
                selected_col=0
                scroll_offset=0
            fi
            ;;
        esac
    done
}

# Main execution
main() {
    # Check if running in Termux
    if [[ -z "$PREFIX" || "$PREFIX" != *"/com.termux/"* ]]; then
        echo "✗ This script must be run inside Termux"
        exit 1
    fi

    # Create .termux directory if needed
    [[ ! -d "$HOME/.termux" ]] && mkdir -p "$HOME/.termux"

    # Backup and set trap
    backup_colors
    trap restore_colors INT TERM

    # Show the menu
    show_menu
}

# Run the script
main "$@"
